{
  "schemaVersion": 1,
  "id": "biomefaker",
  "version": "${version}",

  "name": "biomefaker",
  "description": "Sends a different biome ID for specified biomes to bedrock clients when using GeyserMC",
  "authors": [
    "FireNH"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "LGPL-3.0",
  "icon": "assets/biomefaker/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "fireopal.biomefaker.BiomeFaker"
    ]
  },
  "mixins": [
    "biomefaker.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
